{
  "name": "daniloster.github.io",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.0.1",
  "author": "Danilo Castro <daniloster@gmail.com>",
  "dependencies": {
    "gatsby": "2.3.4",
    "gatsby-image": "2.0.36",
    "gatsby-plugin-manifest": "2.0.26",
    "gatsby-plugin-offline": "2.0.25",
    "gatsby-plugin-react-helmet": "3.0.11",
    "gatsby-plugin-sharp": "2.0.32",
    "gatsby-source-filesystem": "2.0.28",
    "gatsby-transformer-remark": "2.3.8",
    "gatsby-transformer-sharp": "2.1.17",
    "gh-pages": "2.0.1",
    "react-syntax-highlighter": "10.2.1",
    "styled-components": "4.2.0",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.0"
  },
  "devDependencies": {
    "prettier": "1.16.4"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "CONTEXT=production gatsby build",
    "deploy:github": "npm run build && node ./gatsby-ghpages",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daniloster/daniloster.github.io.git"
  }
