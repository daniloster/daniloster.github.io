{
  "name": "daniloster.github.io",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.0.1",
  "author": "Danilo Castro <daniloster@gmail.com>",
  "dependencies": {
    "babel-plugin-styled-components": "1.10.0",
    "gsap": "2.1.2",
    "gatsby": "2.24.11",
    "gatsby-image": "2.4.13",
    "gatsby-plugin-manifest": "2.4.21",
    "gatsby-plugin-offline": "3.2.21",
    "gatsby-plugin-react-helmet": "3.3.10",
    "gatsby-plugin-sharp": "2.6.22",
    "gatsby-plugin-styled-components": "3.3.10",
    "gatsby-source-filesystem": "2.3.23",
    "gatsby-transformer-remark": "2.8.27",
    "gatsby-transformer-sharp": "2.5.11",
    "gh-pages": "2.1.1",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1",
    "react-syntax-highlighter": "11.0.2",
    "styled-components": "4.2.0"
  },
  "devDependencies": {
    "prettier": "1.16.4"
  },
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "build": "CONTEXT=production gatsby build --prefix-paths",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d ./public -b master",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "prestart": "rm -rf public",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daniloster/daniloster.github.io.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/daniloster/daniloster.github.io/issues"
  }
}
